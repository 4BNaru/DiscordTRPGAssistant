from random import uniform
    
class case_group:

	chance = []
	def __init__(self, *args):
		self.cases = args

	def decide_chance(self):
		weight = []
		for i in range(0, len(self.cases)):
			weight.append(uniform(0, 1))
            
		sum = 0
		for i in weight:
			sum += i

		for i in weight:
			self.chance.append(i / sum * 100)
